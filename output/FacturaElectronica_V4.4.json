  "definitions": {
    "FacturaElectronica": {
      "description": "Elemento Raiz de la Facturacion Electrónica",
      "required": [
        "Clave",
        "ProveedorSistemas",
        "CodigoActividadEmisor",
        "NumeroConsecutivo",
        "FechaEmision",
        "Emisor",
        "Receptor",
        "CondicionVenta",
        "ResumenFactura",
        "ds:Signature"
      ],
      "properties": {
        "Clave": {
          "$ref": "#/definitions/ClaveType",
          "description": "Corresponde a la clave del comprobante. Es un campo fijo de cincuenta posiciones y se tiene que utilizar para la consulta del código QR"
        },
        "ProveedorSistemas": {
          "description": "Se debe indicar el número de cedula de identificación del proveedor de sistemas que esté utilizando para la emisión de comprobantes electrónicos",
          "maxLength": 20,
          "type": "string"
        },
        "CodigoActividadEmisor": {
          "description": "Se debe de indicar el código de la actividad económica inscrita a la cual corresponde el comprobante que se está generando",
          "maxLength": 6,
          "minLength": 6,
          "type": "string"
        },
        "CodigoActividadReceptor": {
          "description": "Se debe de indicar el código de la actividad económica inscrita del receptor a la cual corresponden los bienes o servicios que se le están facturando al receptor en caso de ser requerido para un crédito o un gasto deducible.",
          "maxLength": 6,
          "minLength": 6,
          "type": "string"
        },
        "NumeroConsecutivo": {
          "$ref": "#/definitions/NumeroConsecutivoType",
          "description": "Numeración consecutiva del comprobante"
        },
        "FechaEmision": {
          "pattern": "-?([1-9][0-9]{3,}|0[0-9]{3})-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])T(([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\\.[0-9]+)?|(24:00:00(\\.0+)?))(Z|(\\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?",
          "type": "string"
        },
        "Emisor": {
          "$ref": "#/definitions/EmisorType",
          "description": "Emisor del documento"
        },
        "Receptor": {
          "$ref": "#/definitions/ReceptorType",
          "description": "Receptor del documento"
        },
        "CondicionVenta": {
          "description": "Otros",
          "enum": [
            "01",
            "02",
            "03",
            "04",
            "05",
            "06",
            "07",
            "08",
            "10",
            "12",
            "13",
            "14",
            "15",
            "99"
          ],
          "type": "string"
        },
        "CondicionVentaOtros": {
          "description": "Será obligatorio en caso de utilizar el código 99 de \"Otros\" de la nota 5. Se debe describir puntualmente la condición de la venta utilizada.",
          "maxLength": 100,
          "minLength": 5,
          "type": "string"
        },
        "PlazoCredito": {
          "description": "Plazo del crédito, es obligatorio cuando la venta del producto o prestación del servicio sea a crédito",
          "type": "integer"
        },
        "DetalleServicio": {
          "description": "Detalle del Servicio, Mercancía u otro",
          "required": [
            "LineaDetalle"
          ],
          "properties": {
            "LineaDetalle": {
              "oneOf": [
                {},
                {
                  "items": {},
                  "maxItems": 1000,
                  "type": "array"
                }
              ]
            }
          }
        },
        "OtrosCargos": {
          "oneOf": [
            {
              "$ref": "#/definitions/OtrosCargosType"
            },
            {
              "items": {
                "$ref": "#/definitions/OtrosCargosType"
              },
              "maxItems": 15,
              "type": "array"
            }
          ]
        },
        "ResumenFactura": {
          "required": [
            "CodigoTipoMoneda",
            "TotalVenta",
            "TotalVentaNeta",
            "TotalComprobante"
          ],
          "properties": {
            "CodigoTipoMoneda": {
              "$ref": "#/definitions/CodigoMonedaType"
            },
            "TotalServGravados": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total de los servicios gravados con IVA"
            },
            "TotalServExentos": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total de los servicios exentos de IVA"
            },
            "TotalServExonerado": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total servicios exonerados del IVA"
            },
            "TotalServNoSujeto": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Este campo será de condición obligatoria, cuando se seleccionen códigos CAByS que correspondan a un servicio y el servicio sea No Sujeto de IVA"
            },
            "TotalMercanciasGravadas": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total mercancias gravadas con IVA"
            },
            "TotalMercanciasExentas": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total mercancias exentas de IVA"
            },
            "TotalMercExonerada": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total mercancías exoneradas del IVA"
            },
            "TotalMercNoSujeta": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Este campo será de condición obligatoria, cuando se seleccionen códigos CAByS que correspondan a una mercancía y la mercancía sea No Sujeta de IVA"
            },
            "TotalGravado": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total gravado. se obtiene de la suma del total servicios gravados con IV + total mercancias gravadas con IV"
            },
            "TotalExento": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total Exento, se obtiene de la suma de los campos total servicios exentos IV mas total mercancias exentas IV"
            },
            "TotalExonerado": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Se obtiene de la suma de los campos \"total servicios exonerados de IVA\" mas \"total de mercancías exoneradas del IVA\"."
            },
            "TotalNoSujeto": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Se obtiene de la suma de los campos \"Total servicios No Sujetos de IVA\" mas \"Total mercancías No Sujetas de IVA\"."
            },
            "TotalVenta": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Se obtiene de la sumatoria de los campos “total gravado”, “total exento”, “Total Exonerado” y “Total No Sujeto"
            },
            "TotalDescuentos": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Se obtiene de la suma de todos los campo de monto de descuento concedido"
            },
            "TotalVentaNeta": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Se obtiene de la resta de los campos total venta menos total descuento"
            },
            "TotalDesgloseImpuesto": {
              "oneOf": [
                {},
                {
                  "items": {},
                  "maxItems": 1000,
                  "type": "array"
                }
              ]
            },
            "TotalImpuesto": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Se obtiene de la suma de todos campos monto del impuesto"
            },
            "TotalImpAsumEmisorFabrica": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Este campo es de condición obligatoria, cuando existen producto/servicio gravados con algún impuesto en las líneas de detalle que sean asumidos por el emisor"
            },
            "TotalIVADevuelto": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "IVA Devuelto"
            },
            "TotalOtrosCargos": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Total Otros Cargos"
            },
            "MedioPago": {
              "oneOf": [
                {},
                {
                  "items": {},
                  "maxItems": 4,
                  "type": "array"
                }
              ]
            },
            "TotalComprobante": {
              "$ref": "#/definitions/DecimalDineroType",
              "description": "Se obtiene de la suma de los campos \"total venta neta\", \"monto total del impuesto\" y \"total otros cargos\" menos \"total IVA devuelto\", en caso de contar con dichos campos."
            }
          }
        },
        "InformacionReferencia": {
          "oneOf": [
            {},
            {
              "items": {},
              "maxItems": 10,
              "type": "array"
            }
          ]
        },
        "Otros": {
          "properties": {
            "OtroTexto": {
              "oneOf": [
                {},
                {
                  "items": {},
                  "type": "array"
                }
              ]
            },
            "OtroContenido": {
              "oneOf": [
                {},
                {
                  "items": {},
                  "type": "array"
                }
              ]
            }
          }
        },
        "ds:Signature": {
          "oneOf": [
            {
              "$ref": "FORWARD_REFERENCE#/to/http://www.w3.org/2000/09/xmldsig#/ds:Signature"
            },
