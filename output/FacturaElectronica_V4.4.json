      "properties": {
        "TipoDocumentoEX1": {
          "$ref": "#/definitions/TipoExoneracionType",
          "description": "Tipo de documento de exoneración o autorización."
        },
        "TipoDocumentoOTRO": {
          "description": "Será obligatorio en caso de utilizar el código 99 de “Otros” de la nota 10.1. Se debe describir puntualmente el tipo de documento o autorización utilizado",
          "maxLength": 100,
          "minLength": 5,
